node{
	  stage('Checkout')
	

	  {
	

	git branch: 'master',  url: 'https://github.com/chaithrashankarappa/test2.git'
	

	  }
	

	  stage('Build')
	

	     {
	

	         echo "Build the code"
         def mvnHome = tool name: ‘maven-3’, type: ‘maven’
         sh “${mvnHome}”
	

	     }
	

	     stage('Test')
	

	     {
	

	         echo "Test the code"
	

	     }
	

	}

